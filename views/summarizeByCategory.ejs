<h1> Transactions for <%= user.username %></h1>

<p> Here is a table of your transactions. </p>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>category</th>
            <th>amount</th>
        </tr>
    </thead>
	<tbody>
		<% results.forEach(item => { %>
		<tr>
			<td><%= item._id %></td>
			<td><%= item.total %></td>
		</tr>
		<% }); %>
	</tbody>
</table>